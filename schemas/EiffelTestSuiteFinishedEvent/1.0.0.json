{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "meta": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "EiffelTestSuiteFinishedEvent"
          ]
        },
        "version": {
          "type": "string",
          "enum": [
            "1.0.0"
          ],
          "default": "1.0.0"
        },
        "time": {
          "type": "integer"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "source": {
          "$ref": "file:schemas/Definitions/1.0.0.json#/definitions/meta/source"
