        "outcome": {
          "type": "object",
          "properties": {
            "verdict": {
              "type": "string",
              "enum": [
                "PASSED",
                "FAILED",
                "INCONCLUSIVE"
              ]
            },
            "conclusion": {
              "type": "string",
              "enum": [
                "SUCCESSFUL",
                "FAILED",
                "ABORTED",
                "TIMED_OUT",
                "INCONCLUSIVE"
              ]
            },
            "description": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "persistentLogs": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "uri": {
                "type": "string"
              }
            },
            "required": [
              "name",
              "uri"
            ],
            "additionalProperties": false
          }
        },
        "customData": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              },
              "value": {
              }
            },
            "required": [
              "key",
              "value"
            ],
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "links": {
      "$ref": "file:schemas/Definitions/1.0.0.json#/definitions/links"
    }
